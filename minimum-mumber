import java.util.Scanner;
class Min
{
    public static void main(String[] args)
    {
        Scanner sc=new Scanner(System.in);
        int num1=sc.nextInt();
        int num2=sc.nextInt();
        int min;
        if(num1>num2)
        {
            min=num2;
        }
        else
        {
            min=num1;
        }
        System.out.println("Minimum number is:"+min);
    }
}
